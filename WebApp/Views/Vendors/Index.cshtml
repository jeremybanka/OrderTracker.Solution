<!-- prettier-ignore -->
@{ Layout = "_Layout"; } 
@using OrderTracker.Models
<!-- prettier-ignore-end -->

<ul class="vendors">
  @if (Model.Count == 0) {
  <p>No Vendors</p>
  } @foreach (Vendor v in Model) {
  <li>
    <p>@v.Name</p>
    <p>@v.Id</p>
  </li>
  }
  <a href="/vendors/new">Add Vendor</a>
</ul>
